				},
				{
					"color": "#dc3545",
					"name": "Danger",
					"slug": "danger"
				},
				{
					"color": "##c0fecf",
					"name": "Light green",
					"slug": "light-green"
				},
				{
					"color": "#ced4da",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#1a1a31",
					"name": "Gray Dark",
					"slug": "gray-dark"
				},
				{
					"color": "#f9f8f6",
					"name": "Light",
					"slug": "light"
				},
				{
					"color": "#fff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#4A4A4A",
					"name": "Dark",
					"slug": "dark"
				},
				{
					"color": "#000",
					"name": "Black",
					"slug": "black"
