	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"bottom": {
						"style": "solid",
						"width": "0px"
					},
					"left": {
						"style": "solid",
						"width": "0px"
					},
					"radius": "0px",
					"right": {
						"style": "solid",
						"width": "0px"
					},
					"top": {
						"style": "solid",
						"width": "0px"
					},
					"width": "0px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			},
			"core/latest-comments": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/latest-posts": {
				"spacing": {
					"padding": {
						"left": "0px"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-excerpt": {
				"elements": {
					"link": {
						"border": {
							"radius": "4px"
						},
						"color": {
							"background": "var(--wp--preset--color--light-grey)"
						},
						"spacing": {
							"padding": {
								"bottom": "calc(.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(.667em + 2px)"
							}
						}
					}
				}
			},
			"core/post-featured-image": {
				"spacing": {
					"margin": {
						"bottom": "2.5rem"
					}
				}
			},
			"core/post-navigation-link": {
				"elements": {
					"link": {
						"border": {
							"radius": "40px"
						},
						"color": {
							"background": "var(--wp--preset--color--light-grey)"
						},
						"spacing": {
							"padding": {
								"bottom": "calc(.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(.667em + 2px)"
							}
						}
					}
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						"border": {
							"radius": "4px"
						},
						"color": {
							"background": "var(--wp--preset--color--light-grey)"
						},
						"spacing": {
							"padding": {
								"bottom": "calc(.667em + 2px)",
								"left": "calc(1.333em + 2px)",
								"right": "calc(1.333em + 2px)",
								"top": "calc(.667em + 2px)"
							}
						}
					}
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"bottom": "0px"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-black)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			},
			"h1": {
				"color": {
					"text": "var(--wp--preset--color--gray-dark)"
				},
