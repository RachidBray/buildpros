				{
					"color": "#101111",
					"name": "Dark Black",
					"slug": "dark-black"
				}
			]
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": null
		},
		"spacing": {
			"blockGap": true,
			"padding": true,
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Gabarito",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400 900",
							"src": [
								"file:./assets/fonts/gabarit/Gabarito-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Gabarito\", sans-serif",
					"name": "Gabarito",
					"slug": "gabarito"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Adventpro",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/adventpro/AdventPro-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Advent Pro\", sans-serif",
					"name": "Adventpro",
					"slug": "adventpro"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "1.1rem",
					"slug": "normal"
				},
				{
					"name": "medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"name": "Extra large",
					"size": "3rem",
					"slug": "extra-large"
				}
			]
		}
